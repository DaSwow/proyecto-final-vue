a(idpeli) {\n      alert(\"Película a editar: \" + idpeli);\n    },\n    async listarTodo() {\n      try {\n        this.axios.get(\"/\").then(response => {\n          this.peliculas = response.data;\n        });\n      } catch (error) {\n        console.error;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";AAqHA,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,SAAS,EAAE;QACTC,EAAE,EAAE,EAAE;QACNC,MAAM,EAAE,EAAE;QACVC,MAAM,EAAE,EAAE;QACVC,UAAU,EAAE;MACd,CAAC;MACDC,oBAAoB,EAAC;QACnBJ,EAAE,EAAE,EAAE;QACNC,MAAM,EAAE,EAAE;QACVC,MAAM,EAAE,EAAE;QACVC,UAAU,EAAE;MACd,CAAC;MACDE,SAAS,EAAE,EAAE;MACbC,MAAM,EAAE,KAAK;MACbC,KAAK,EAAE,CAAC;MACRC,MAAM,EAAE;IAEV;EACF,CAAC;EAED;EACAC,OAAO,GAAG;IACR,IAAI,CAACC,WAAW,EAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACP;IACA;IACAC,aAAa,CAACT,UAAU,EAAE;MACxB,IAAIA,UAAS,KAAM,IAAI,EAAE;QACvB,OAAO,IAAG;MACZ,OAAO;QACL,OAAO,IAAG;MACZ;IACF,CAAC;IACD,MAAMO,WAAW,GAAG;MAClB;MACA,IAAI,CAACG,KAAK,CAACC,GAAG,CAAC,GAAG,CAAC,CAACC,IAAI,CAACC,GAAE,IAAK;QAC9B;QACA;QACA,IAAI,CAACX,SAAQ,GAAIW,GAAG,CAACnB,IAAI;MAC3B,CAAC,CAAC,CAACoB,KAAK,CAACC,KAAI,IAAK;QAChBC,OAAO,CAACC,GAAG,CAACF,KAAK;MACnB,CAAC;IAEH,CAAC;IAED,MAAMG,eAAe,GAAG;MACtB,IAAI;QACF,IAAI,IAAI,CAAClB,UAAS,IAAK,EAAE,EAAE;UAAE,IAAI,CAACA,UAAS,GAAI,IAAG;QAAE,OAC/C;UAAE,IAAI,CAACA,UAAS,GAAI,KAAI;QAAE;QAC/B,IAAI,CAACU,KAAK,CAACS,IAAI,CAAC,YAAY,EAAE,IAAI,CAACvB,SAAS,EACzCgB,IAAI,CAAEC,GAAG,IAAK;UACb,IAAI,CAAClB,SAAS,CAACyB,IAAI,CAACP,GAAG,CAACnB,IAAI,CAAC;UAC7B,IAAI,CAACE,SAAS,CAACC,EAAC,GAAI,EAAC;UACrB,IAAI,CAACD,SAAS,CAACE,MAAK,GAAI,EAAC;UACzB,IAAI,CAACF,SAAS,CAACG,MAAK,GAAI,EAAC;UACzB,IAAI,CAACH,SAAS,CAACI,UAAS,GAAI,KAAI;UAChC,IAAI,CAACO,WAAW,EAAC;QAEnB,CAAC;MACL,EAAE,OAAOQ,KAAK,EAAE;QACdM,KAAK,CAAC,6BAA6B;QACnCL,OAAO,CAACD,KAAK,CAACA,KAAK;MACrB;IAEF,CAAC;IACDO,aAAa,CAACzB,EAAE,EAAEO,KAAK,EAAE;MACvB,IAAI,CAACC,MAAK,GAAIR,EAAC;MACf,IAAI,CAACM,MAAK,GAAI,IAAG;MACjB,IAAI,CAACC,KAAI,GAAIA,KAAI;IACnB,CAAC;IACD,MAAMmB,gBAAgB,CAACC,MAAM,EAAE;MAC7BH,KAAK,CAAC,sBAAqB,GAAIG,MAAM;IACvC,CAAC;IACD,MAAMC,cAAc,